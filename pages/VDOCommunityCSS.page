    /*Retail Application CSS*/
        .retail{
            margin:0px;
            padding:0px;
        }
        .retail .logo{
            width:100%;
            height:55px;
            background: url('{!URLFOR($Resource.VDORetailResources,'images/logo.png')}') no-repeat;
            background-size: auto 35px;
            background-position:76px center;
            box-shadow: 0 2px 4px rgba(0,0,0,.07);
            padding:10px 0px 10px 76px;
            z-index: 20;
            position: relative;
            box-sizing:border-box;
        }
        .retail .logo img{
            float: right;
            width: 35px;
            height: 35px;
            margin-right: 20px;
            border-radius: 35px;
        }
        .retail .content .store{
            width:100%;
        }
        .retail .content .store .header{
            width:100%;
            background-color: #f4f6f9;
            height:85px;
            padding:20px;
            box-sizing:border-box;
            position:relative;
            z-index:10;
        }
        .retail .content .store .details{
            width:100%;
            border-bottom: 1px solid #d8dde6;
